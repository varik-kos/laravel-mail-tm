{
    "name": "valery/laravel-mail-tm",
    "description": "Laravel package for creating temporary email accounts - Mail.tm",
    "homepage": "https://github.com/varik-kos/laravel-mail-tm",
    "keywords": ["mailtm"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0|^8.1",
        "guzzlehttp/guzzle": "^7.5.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Valery\\LaravelMailTm\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Valery",
            "email": "magvai75@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Valery\\LaravelMailTm\\MailTmServiceProvider"
            ],
            "aliases": {
                "MailTm": "Valery\\LaravelMailTm\\MailTmFacade"
            }
        }
    }
}
